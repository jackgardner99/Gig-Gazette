Table Managers {
	id int [pk]
	name varchar
	email varchar
}

Table Band {
	id int [pk]
	bandName varchar
	genreId int [ref: > Genre.id]
	managerId int [ref: > Managers.id]
}

Table BandMember {
	id int [pk]
	name varchar
	bandId int [ref: > Band.id]
}

Table Artist {
    id int [pk]
    artistName varchar
    genreId int [ref: > Genre.id]
	managerId int [ref: > Managers.id]
}

Table Genre {
	id int [pk]
	genreName varchar
}

Table Venues {
	id int [pk]
	name varchar
	location varchar
	link url
}

Table BandShow {
	id int [pk]
	bandId int [ref: > Band.id]
	venueId int [ref: > Venues.id]
	link url
	showDate date
}

Table ArtistShow {
	id int [pk]
	artistId int [ref: > Artist.id]
	venueId int [ref: > Venues.id]
	link url
	showDate date
}

Table MusicPlatforms {
	id int [pk]
	name varchar
}

Table ArtistMusic {
	id int [pk]
	artistId int [ref: - Artist.id]
	musicLink url
	platformId int [ref: > MusicPlatforms.id]
}

Table BandMusic {
	id int [pk]
	bandId int [ref: - Band.id]
	musicLink url
	platformId int [ref: > MusicPlatforms.id]
}
